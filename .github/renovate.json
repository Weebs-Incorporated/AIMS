{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "rangeStrategy": "bump",
    "dependencyDashboard": false,
    "ignorePaths": ["**/node_modules/**"],
    "packageRules": [
        {
            "matchPackagePatterns": ["eslint"],
            "groupName": "eslint packages"
        }
    ]
}
